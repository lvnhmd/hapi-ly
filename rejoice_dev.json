{
    "connections": [
        {
            "port": 8080,
            "labels": [
                "http"
            ]
        },
        {
            "port": 8088,
            "labels": [
                "api"
            ]
        }
    ],
    "plugins": {
        "bell": [
            {
                "select": "http"
            }
        ],
        "hapi-auth-cookie": [
            {
                "select": "http"
            }
        ],
        "./authentication": [
            {
                "select": "http"
            }
        ],
        "./controllers": [
            {
                "select": [
                    "http",
                    "api"
                ]
            }
        ],
        "./models": [
            {
                "select": [
                    "http",
                    "api"
                ]
            }
        ],
        "./routes": [
            {
                "select": [
                    "http"
                ]
            }
        ],
        "./api": [
            {
                "select": [
                    "api"
                ]
            }
        ],
        "good": {
            "opsInterval": 5000,
            "reporters": [
                {
                    "reporter": "good-console",
                    "events": {
                        "ops": "*",
                        "log": "*"
                    }
                }
            ]
        },
        "blipp": [
            {}
        ]
    }
}